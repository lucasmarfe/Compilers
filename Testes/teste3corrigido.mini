program teste3corrigido
    integer cont, qtd;
    real media, altura, soma;
   {
      cont := 5;
      soma := 0.0;
repeat
        write("Altura: ");
        read (altura);
        soma := soma + altura;
        cont := cont - 1;
      until(cont+qtd);
      write("Media: ");
      write (soma / qtd);
}